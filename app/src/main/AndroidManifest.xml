<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.cityinformation">

    <uses-feature
        android:name="android.hardware.telephony"
        android:required="false" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
<uses-permission android:name="android.permission.CALL_PHONE"/>
    <application
        android:allowBackup="true"
        android:allowClearUserData="false"
        android:allowClearUserDataOnFailedRestore="false"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/city"
        android:label="@string/app_name"
        android:roundIcon="@drawable/city"
        android:supportsRtl="true"
        android:theme="@style/Theme.CityInformation"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".User.ContactingActivity"
            android:exported="false" />
        <activity
            android:name=".User.LastActivity"
            android:exported="false" />
        <activity
            android:name=".User.StagesActivity"
            android:exported="false" />
        <activity
            android:name=".User.CityView"
            android:exported="false" />
        <activity
            android:name=".Admin.AnotherSegment"
            android:exported="false" />
        <activity
            android:name=".Admin.WriteCity"
            android:exported="false" />
        <activity
            android:name=".Admin.AddPlaces"
            android:exported="false" />
        <activity
            android:name=".User.CitySelecting"
            android:exported="false" />
        <activity
            android:name=".SignUp"
            android:exported="false" />
        <activity
            android:name=".LoginPage"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Admin.ViewAdmin" />
    </application>

</manifest>